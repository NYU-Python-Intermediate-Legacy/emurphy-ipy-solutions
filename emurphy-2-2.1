
fh = open("bitly.tsv")
fh.readline()


city_set = set()
print type(city_set)

for line in fh:
   split_line = line.rstrip().split("\t")
   if len(split_line) > 4:
      if split_line[3].isalpha() and len(split_line[3]) > 1:
         city_set.add(split_line[3])

#Sorted unique city list

city_list = list(city_set)
city_list.sort()
print city_list
print len(city_list)
